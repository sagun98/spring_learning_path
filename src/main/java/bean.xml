<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id ="Person" class="com.example.main.Person" init-method="init" destroy-method="destroy">
	<property name="gender" value="Male"/>
</bean>

<bean id="address" class="com.example.main.Address">
	<property name = "street" value="123 Filhiol Ave"/>
	<property name = "cityState" value="MA"/>
</bean>


</beans>
